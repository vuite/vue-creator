{
  "name": "@vuite/vue-creator",
  "version": "1.0.1",
  "description": "A Command-Line tool for creating VueJS (2 & 3) Component/Directive file(s)",
  "keywords": [
    "cli",
    "cli-tool",
    "vue cli tool",
    "vue component creator",
    "vue directive creator",
    "vue generator",
    "vue 2",
    "vue 3"
  ],
  "license": "MIT",
  "main": "./bin/vuec",
  "types": "./types/index.d.ts",
  "bin": {
    "vuec": "./bin/vuec",
    "vue-create": "./bin/vuec"
  },
  "files": [
    "bin",
    "lib",
    "types",
    "templates"
  ],
  "scripts": {
    "dev": "node bin/vuec",
    "prepack": "rimraf lib types && tsc",
    "types": "rimraf types && tsc --emitDeclarationOnly",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "ava"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/listr": "^0.14.4",
    "@types/node": "^17.0.10",
    "@types/pug": "^2.0.6",
    "ava": "^4.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vue": "2.6",
    "xo": "^0.47.0"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "clear": "^0.1.0",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "listr": "^0.14.3",
    "merge": "^2.1.1",
    "pug": "^3.0.2"
  },
  "peerDependencies": {
    "typescript": "^3.0 || ^4.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vuite/vue-creator.git"
  },
  "author": {
    "name": "Jonson",
    "email": "www.jbc.bd@gmail.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "xo": {
    "extensions": [
      ".ts"
    ],
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "object-curly-spacing": "off",
      "@typescript-eslint/object-curly-spacing": "off"
    }
  }
}
